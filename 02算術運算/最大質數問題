#include <stdio.h>

int isPrime(int x) {
    if (x < 2) return 0;
    for (int i = 2; i * i <= x; i++) {
        if (x % i == 0)
            return 0;
    }
    return 1;
}

int main() {
    int N;
    scanf("%d", &N);

    for (int i = N - 1; i >= 2; i--) {
        if (isPrime(i)) {
            printf("%d\n", i);
            break;
        }
    }

    return 0;
}
