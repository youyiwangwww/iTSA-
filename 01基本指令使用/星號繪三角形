#include <stdio.h>

int main() {
    int type;
    if (scanf("%d", &type) != 1) return 0;

    //空心三角形
    if (type == 1) {
        for (int i = 1; i <= 5; i++) {
            for (int j = 1; j <= 5 - i; j++) printf(" ");
            if (i == 1) {
                printf("*\n");
            } 
            else if (i == 5) {
                for (int j = 1; j <= 9; j++) printf("*");
                printf("\n");
            } 
            else {
                printf("*"); //左邊界
                for (int j = 1; j <= 2 * i - 3; j++) printf(" ");
                printf("*\n");//右邊界
            }
        }
    } 
    else if (type == 2) {
        //實心三角形
        for (int i = 1; i <= 5; i++) {
            for (int j = 1; j <= 5 - i; j++) printf(" ");
            for (int j = 1; j <= 2 * i - 1; j++) printf("*");
            printf("\n");
        }
    } 
    else if (type == 3) {
        //實心倒三角形
        for (int i = 1; i <= 5; i++) {
            for (int j = 1; j <= i - 1; j++) printf(" ");
            for (int j = 1; j <= 2 * (6 - i) - 1; j++) printf("*");
            printf("\n");
        }
    }
    
    return 0;
}
