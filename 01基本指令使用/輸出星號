#include <stdio.h>

int main() {
    int num;
    int digits[5];

    scanf("%d", &num);

    //23416%10=6 23416/10=2341
    for (int i = 4; i >= 0; i--) {
        digits[i] = num % 10;
        num /= 10;
    }

    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < digits[i]; j++) {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
