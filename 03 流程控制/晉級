#include <stdio.h>

int main() {
    int n, score, pass = 0;
    char name[20];

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%s %d", name, &score);
        if (score >= 60) {
            printf("%s\n", name);
            pass++;
        }
    }

    if (pass * 2 >= n)
        printf("晉級\n");
    else
        printf("失敗\n");

    return 0;
}
