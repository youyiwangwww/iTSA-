#include <stdio.h>

int main() {
    long long N;
    int prime = 1;

    scanf("%lld", &N);

    if (N <= 1)
        prime = 0;
    else {
        for (long long i = 2; i * i <= N; i++) {
            if (N % i == 0) {
                prime = 0;
                break;
            }
        }
    }

    if (prime)
        printf("YES\n");
    else
        printf("NO\n");

    return 0;
}
