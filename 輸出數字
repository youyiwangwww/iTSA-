#include <stdio.h>

int main() {
    int num;
    while (scanf("%d", &num) != EOF) {
        int thousands = num / 1000;       //2314/1000=2   
        int hundreds = (num / 100) % 10;     //2314/100=23 23%10=3
        int tens = (num / 10) % 10;          
        int ones = num % 10;                 

        printf("%d\n", thousands);
        printf("%d\n", hundreds);
        printf("%d\n", tens);
        printf("%d\n", ones);
    }

    return 0;
}
